<?xml version="1.0" encoding="UTF-8"?>
  <hibernate-mapping namespace="Domain" assembly="Domain" xmlns="urn:nhibernate-mapping-2.2">
    <class table="contact" name="Contact">
      <id name="Id" type="int" column="id">
        <generator class="native"/>
      </id>
      <property name="Nom" column="nom" not-null="true"/>
      <property name="Lien" column="lien" not-null="true"/>
      <property name="IdUtilisateur" column="id_utilisateur" not-null="true" type="int"/>
    </class>
  </hibernate-mapping>